  s = slld2011[i,2]
  rmSpec <- "\xca" 
  slld2011[i,2] <- gsub(rmSpec, "Ê", s)   
}


for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xc7" 
  slld2011[i,2] <- gsub(rmSpec, "Ç", s)   
}


for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xd4" 
  slld2011[i,2] <- gsub(rmSpec, "Ô", s)   
}

for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xdf" 
  slld2011[i,2] <- gsub(rmSpec, "ß", s)   
}


for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xd6" 
  slld2011[i,2] <- gsub(rmSpec, "Ö", s)   
}


