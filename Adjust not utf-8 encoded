

for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xc0" 
  slld2011[i,2] <- gsub(rmSpec, "", s) # gsub replace any matches in remSpec and replace them with "".
}


for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xd9" 
  slld2011[i,2] <- gsub(rmSpec, "Ù", s)  # gsub replace any matches in remSpec and replace them with "".
  
}

for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xc0" 
  slld2011[i,2] <- gsub(rmSpec, "À", s) # gsub replace any matches in remSpec and replace them with "".
  
}


for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xc8" 
  slld2011[i,2] <- gsub(rmSpec, "È", s) # gsub replace any matches in remSpec and replace them with "".
}



for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xcc" 
  slld2011[i,2] <- gsub(rmSpec, "Ì", s)   # gsub replace any matches in remSpec and replace them with "".
  }

for (i in 1:nrow(slld2011)){
  s = slld2011[i,2]
  rmSpec <- "\xd2" 
  slld2011[i,2] <- gsub(rmSpec, "Ò", s)  
}

